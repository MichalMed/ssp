    ], [ a owl:Restriction;
      owl:allValuesFrom d-sgov-právní-akty-pojem:Právní-akt;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek-1
    ], [ a owl:Restriction;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek-2;
      owl:someValuesFrom d-sgov-právní-akty-pojem:Znění-právního-aktu
    ], [ a owl:Restriction;
      owl:allValuesFrom d-sgov-právní-akty-pojem:Znění-právního-aktu;

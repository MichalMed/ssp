@prefix g-sgov-částky: <https://slovník.gov.cz/generický/částky/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix g-sgov-částky-pojem: <https://slovník.gov.cz/generický/částky/pojem/> .

<https://slovník.gov.cz/generický/věci/pojem/věc>
        a       owl:Class .

<https://slovník.gov.cz/generický/číselníky/pojem/měna>
        a       owl:Class .

g-sgov-částky:model  a  <http://onto.fel.cvut.cz/ontologies/slovník/agendový/popis-dat/pojem/model> , owl:Ontology ;
        <http://purl.org/dc/terms/created>
                "2020-07-28" ;
        <http://purl.org/dc/terms/title>
                "Generický slovník peněžních částek - model"@cs ;
        <http://purl.org/ontology/bibo/status>
                "Specification"@en , "Specifikace"@cs ;
        <http://purl.org/vocab/vann/preferredNamespacePrefix>
                "g-sgov-částky-pojem" ;
        <http://purl.org/vocab/vann/preferredNamespaceUri>
                "https://slovník.gov.cz/generický/částky/pojem/" ;
        owl:imports     g-sgov-částky:glosář ;
        owl:versionIRI  <https://slovník.gov.cz/generický/částky/model/verze/0.0.1> .

<https://slovník.gov.cz/základní/pojem/má-vztažený-prvek>
        a              owl:ObjectProperty ;
        owl:inverseOf  <https://slovník.gov.cz/základní/pojem/je-ve-vztahu> .

<https://slovník.gov.cz/základní/pojem/má-vlastnost>
        a       owl:ObjectProperty .

g-sgov-částky-pojem:výše
        a                owl:Class , <https://slovník.gov.cz/základní/pojem/typ-vlastnosti> ;
        rdfs:subClassOf  <https://slovník.gov.cz/základní/pojem/vlastnost> ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      <https://slovník.gov.cz/základní/pojem/je-vlastností> ;
                           owl:someValuesFrom  [ a            owl:Class ;
                                                 owl:unionOf  ( g-sgov-částky-pojem:jednotková-částka g-sgov-částky-pojem:částka )
                                               ]
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  [ a            owl:Class ;
                                                owl:unionOf  ( g-sgov-částky-pojem:jednotková-částka g-sgov-částky-pojem:částka )
                                              ] ;
                           owl:onProperty     <https://slovník.gov.cz/základní/pojem/je-vlastností>
                         ] .

<https://slovník.gov.cz/základní/pojem/je-vlastností>
        a              owl:ObjectProperty ;
        owl:inverseOf  <https://slovník.gov.cz/základní/pojem/má-vlastnost> .

<https://slovník.gov.cz/generický/číselníky/pojem/jednotka>
        a       owl:Class .

g-sgov-částky-pojem:má-jednotku
        a                owl:Class , <https://slovník.gov.cz/základní/pojem/typ-vztahu> ;
        rdfs:subClassOf  <https://slovník.gov.cz/základní/pojem/vztah> ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      <https://slovník.gov.cz/základní/pojem/má-vztažený-prvek-2> ;
                           owl:someValuesFrom  <https://slovník.gov.cz/generický/číselníky/pojem/jednotka>
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  <https://slovník.gov.cz/generický/číselníky/pojem/jednotka> ;
                           owl:onProperty     <https://slovník.gov.cz/základní/pojem/má-vztažený-prvek-2>
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      <https://slovník.gov.cz/základní/pojem/má-vztažený-prvek-1> ;
                           owl:someValuesFrom  g-sgov-částky-pojem:jednotková-částka
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  g-sgov-částky-pojem:jednotková-částka ;
                           owl:onProperty     <https://slovník.gov.cz/základní/pojem/má-vztažený-prvek-1>
                         ] .

<https://slovník.gov.cz/základní/pojem/má-vztažený-prvek-2>
        a                   owl:ObjectProperty ;
        rdfs:subPropertyOf  <https://slovník.gov.cz/základní/pojem/má-vztažený-prvek> .

<https://slovník.gov.cz/základní/pojem/je-ve-vztahu>
        a       owl:ObjectProperty .

<https://slovník.gov.cz/základní/pojem/má-vztažený-prvek-1>
        a                   owl:ObjectProperty ;
        rdfs:subPropertyOf  <https://slovník.gov.cz/základní/pojem/má-vztažený-prvek> .

g-sgov-částky-pojem:částka
        a                <https://slovník.gov.cz/základní/pojem/typ-objektu> , owl:Class ;
        rdfs:subClassOf  <https://slovník.gov.cz/generický/věci/pojem/věc> .

g-sgov-částky-pojem:má-měnu
        a                owl:Class , <https://slovník.gov.cz/základní/pojem/typ-vztahu> ;
        rdfs:subClassOf  <https://slovník.gov.cz/základní/pojem/vztah> ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      <https://slovník.gov.cz/základní/pojem/má-vztažený-prvek-2> ;
                           owl:someValuesFrom  <https://slovník.gov.cz/generický/číselníky/pojem/měna>
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  <https://slovník.gov.cz/generický/číselníky/pojem/měna> ;
                           owl:onProperty     <https://slovník.gov.cz/základní/pojem/má-vztažený-prvek-2>
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      <https://slovník.gov.cz/základní/pojem/má-vztažený-prvek-1> ;
                           owl:someValuesFrom  g-sgov-částky-pojem:částka
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  g-sgov-částky-pojem:částka ;
                           owl:onProperty     <https://slovník.gov.cz/základní/pojem/má-vztažený-prvek-1>
                         ] .

g-sgov-částky-pojem:jednotková-částka
        a                <https://slovník.gov.cz/základní/pojem/typ-objektu> , owl:Class ;
        rdfs:subClassOf  g-sgov-částky-pojem:částka .

z-sgov-pojem:je-instancí-typu
        a            owl:ObjectProperty ;
        rdfs:domain  [ a            owl:Class ;
                       owl:unionOf  ( z-sgov-pojem:vztah
                                      [ a            owl:Class ;
                                        owl:unionOf  ( z-sgov-pojem:vlastnost
                                                       [ a            owl:Class ;
                                                         owl:unionOf  ( z-sgov-pojem:objekt
                                                                        [ a            owl:Class ;
                                                                          owl:unionOf  ( z-sgov-pojem:proměnný-prvek z-sgov-pojem:prvek )
                                                                        ]
                                                                      )
                                                       ]
                                                     )
                                      ]
                                    )
                     ] ;
        rdfs:label   "je instancí typu"@cs ;
        rdfs:range   [ a            owl:Class ;
                       owl:unionOf  ( z-sgov-pojem:typ-vztahu
                                      [ a            owl:Class ;
                                        owl:unionOf  ( z-sgov-pojem:typ-vlastnosti
                                                       [ a            owl:Class ;
                                                         owl:unionOf  ( z-sgov-pojem:typ-objektu
                                                                        [ a            owl:Class ;
                                                                          owl:unionOf  ( z-sgov-pojem:typ-proměnného-prvku z-sgov-pojem:typ )
                                                                        ]
                                                                      )
                                                       ]
                                                     )
                                      ]
                                    )
                     ] .

